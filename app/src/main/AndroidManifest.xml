<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="itms.com.pe.app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="android.support.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@drawable/logoapp"
        android:label="CheckHealth"
        android:supportsRtl="true"
        android:theme="@style/ThemesinActionBAR"
        tools:replace="android:icon,android:theme,android:label">
        <activity
            android:name=".SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".login"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Registro"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Prueba"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MenuOpciones"
            android:label="@string/title_activity_menu_opciones"
            android:screenOrientation="portrait"
            android:theme="@style/ThemesinActionBAR" />
        <activity
            android:name=".registro_first_paciente"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyAdV7WuZXSpawgLEgAJxWXrBq2okAC2bzQ" />

        <activity
            android:name=".confirmacion_servicio"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Translucent" />
        <activity android:name=".servicios_Domicilab"></activity>
    </application>

</manifest>